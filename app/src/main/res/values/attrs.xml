<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SearchView">
        <attr name="hintText" format="string"/>
        <attr name="backIcon" format="reference"/>
        <attr name="oneKeyCleanIcon" format="reference"/>
        <attr name="historyIcon" format="reference"/>
        <attr name="searchDefaultState" format="enum">
            <enum name="close" value="0"/>
            <enum name="open" value="1"/>
        </attr>
        <attr name="historyTextColor" format="color"/>
        <attr name="inputView_height" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="MaterialSearchView">
        <attr name="searchBackground" format="integer" />
        <attr name="searchVoiceIcon" format="integer" />
        <attr name="searchCloseIcon" format="integer" />
        <attr name="searchBackIcon" format="integer" />
        <attr name="searchSuggestionIcon" format="integer" />
        <attr name="searchSuggestionBackground" format="integer" />
        <attr name="android:hint" />
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:inputType" />
    </declare-styleable>
</resources>